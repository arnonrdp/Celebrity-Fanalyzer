{
  "hosting": {
    "headers": [
      {
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ],
        "source": "/service-worker.js"
      }
    ],
    "ignore": ["**/.*", "**/node_modules/**"],
    "public": "dist/pwa"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
